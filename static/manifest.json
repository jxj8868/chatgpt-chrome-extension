{
  "manifest_version": 3,
  "name": "ChatEase",
  "version": "1.0",
  "description": "轻松聊天,ChatEase",
  "permissions": ["sidePanel", "storage","script ing", "contextMenus", "notifications", "webRequest", "https://wxchatbot-east-us.openai.azure.com/*"],
  "action": {
    "default_title": "ChatEase",
    "default_icon": {
      "16": "icons/icon.png",
      "32": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "default_popup": "popup.html"
  },
  "side_panel":{
    "default_path": "sidebar.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "key": ""
}